import java.util.Scanner;
public class Solution{
    public static void main(String[]args)
    {
        Scanner in=new Scanner(System.in);
        int n=in.nextInt();
        int k=in.nextInt();
        int q=in.nextInt();
        
        
        int[]arr=new int[n];
        for(int i=0;i<n;i++)
        {
            arr[(i+k)%n]=in.nextInt();
        }
        
        for(int i=0;i<q;i++)
        {
            System.out.println(arr[in.nextInt()]);
        }
    }
    
}
